<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>jlatex</groupId>
	<artifactId>jlatex</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>pom</packaging>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<version.jre>11</version.jre>
		<version.junit>5.9.3</version.junit>
	</properties>
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.8.1</version>
				<configuration>
					<source>${version.jre}</source>
					<target>${version.jre}</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.junit.jupiter</groupId>
				<artifactId>junit-jupiter</artifactId>
				<type>pom</type>
				<version>${version.junit}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
	<modules>
		<module>jlatex.api</module>
		<module>jlatex.bibtex</module>
		<module>jlatex.samples</module>
	</modules>
	<scm>
		<connection>scm:git:https://github.com/uthanda/JLatex.git</connection>
		<url>https://github.com/uthanda/JLatex.git</url>
		<tag>HEAD</tag>
	</scm>
	<distributionManagement>
		<repository>
			<id>github</id>
			<name>Michael Oland</name>
			<url>https://maven.pkg.github.com/uthanda/JLatex</url>
		</repository>
	</distributionManagement>
</project>
